#!/usr/bin/env xdg-open
[Desktop Entry]
Version=1.0
Terminal=false
Type=Application
Name=Chrome App Launcher (Personal)
Exec=/opt/google/chrome/google-chrome --show-app-list --user-data-dir=.config/google-chrome/Default
Icon=chrome-app-list
Categories=Network;WebBrowser;
StartupWMClass=chrome_app_list
